      <mj-column>
        <mj-text align="center">
          &copy; Shopify, 151 O'Connor Street, Ground floor, Ottawa, ON, Canada, K2P 2L8
        </mj-text>
      </mj-column>
    </mj-section>

    <mj-section>
      <mj-column>
        <mj-text align="center" padding="0" width="0">
